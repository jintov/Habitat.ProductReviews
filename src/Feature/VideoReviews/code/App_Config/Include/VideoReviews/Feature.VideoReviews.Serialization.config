<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.VideoReviews" description="Feature Video Reviews" dependencies="" patch:after="configuration[@name='Foundation.Serialization']">
          <targetDataStore physicalRootPath="$(sourceFolder)\feature\videoreviews\serialization" type="Rainbow.Storage.SerializationFileSystemDataStore, Rainbow" useDataCache="false" singleInstance="true" />
          <predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
            <include name="Feature.VideoReviews.Templates" database="master" path="/sitecore/templates/Feature/VideoReviews" />
            <include name="Feature.VideoReviews.Renderings" database="master" path="/sitecore/layout/renderings/Feature/VideoReviews" />
          </predicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>